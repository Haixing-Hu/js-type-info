{
  "name": "@haixing_hu/type-info",
  "version": "1.0.2",
  "description": "A JavaScript library provides a type information detection function",
  "author": "Haixing Hu",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-type-info.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-type-info/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-type-info",
  "main": "dist/type-info.cjs.min.js",
  "module": "dist/type-info.esm.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/type-info.cjs.min.js",
      "import": "./dist/type-info.esm.min.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "yarn clean && yarn doc && yarn build:dev && yarn build",
    "test:es5": "cross-env NODE_ENV=test TARGETS='fully supports es5' jest",
    "test:es6": "cross-env NODE_ENV=test TARGETS='fully supports es6' jest",
    "test:defaults": "cross-env NODE_ENV=test TARGETS='defaults' jest",
    "test": "yarn test:es5 && yarn test:es6 && yarn test:defaults",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version --es-version=5 --show-error=true --target-dir=dist",
    "deploy": "yarn && yarn lint && yarn test && yarn build:all && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.23.3",
    "@haixing_hu/eslint-config": "^1.3.0",
    "@haixing_hu/jsdoc-minami": "^1.5.0",
    "@haixing_hu/rollup-builder": "^1.0.7",
    "@jest/core": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.4.2",
    "core-js": "^3.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^1.1.0",
    "jsdoc": "^3.6.7",
    "jsdom": "^18.1.1",
    "rollup": "^4.1.4",
    "rollup-plugin-analyzer": "^4.0.0"
  },
  "packageManager": "yarn@4.0.0"
}
